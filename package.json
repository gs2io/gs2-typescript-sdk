{
  "name": "gs2",
  "version": "1.0.2",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "axios": "^0.26.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.18",
    "typescript": "^4.5.5",
    "tslib": "^2.3.1",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.3",
    "@types/jest": "^27.4.0"
  },
  "types": "index.d.ts",
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "test": "jest",
  "jest": {
    "moduleNameMapper": {
      "@/gs2": "<rootDir>/src",
      "@/gs2/(.*)$": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "testURL": "https://gs2.io/",
    "verbose": true
  }
}
