{
  "name": "gs2-typescript-sdk",
  "version": "1.1.1",
  "private": true,
  "scripts": {},
  "dependencies": {
    "axios": "^0.19.0"
  },
  "devDependencies": {
    "@types/node": "^15.6.1",
    "typescript": "^3.7.4",
    "tslib": "^1.11.2",
    "jest": "^25.5.4",
    "ts-jest": "^25.4.0",
    "@types/jest": "^25.2.1"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "test": "jest",
  "jest": {
    "moduleNameMapper": {
      "@/gs2": "<rootDir>/src",
      "@/gs2/(.*)$": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "testURL": "https://gs2.io/",
    "verbose": true
  }
}
